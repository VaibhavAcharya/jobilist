{
  "name": "remix-template-remix",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "yarn run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/tailwind.css -o app/styles/tailwind.css",
    "dev": "concurrently \"yarn run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./styles/tailwind.css -o app/styles/tailwind.css",
    "start": "remix-serve build",
    "format": "prettier --ignore-path .gitignore --write ."
  },
  "dependencies": {
    "@headlessui/react": "^1.6.0",
    "@prisma/client": "^4.7.1",
    "@radix-ui/react-slider": "^0.1.4",
    "@remix-run/node": "^1.4.1",
    "@remix-run/react": "^1.4.1",
    "@remix-run/serve": "^1.4.1",
    "cloudinary": "^1.29.1",
    "date-fns": "^2.28.0",
    "razorpay": "^2.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.4.1",
    "@remix-run/eslint-config": "^1.4.1",
    "autoprefixer": "^10.4.5",
    "concurrently": "^7.1.0",
    "eslint": "^8.11.0",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "prisma": "^4.7.1",
    "tailwindcss": "^3.0.24",
    "tailwindcss-radix": "^2.2.0"
  },
  "engines": {
    "node": ">=14"
  }
}
